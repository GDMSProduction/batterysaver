<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@drawable/grey"
    tools:context=".CloseList">


  <Button
      android:id="@+id/Load"
      android:layout_width="72dp"
      android:layout_height="61dp"
      android:layout_marginBottom="8dp"
      android:layout_marginEnd="8dp"
      android:layout_marginStart="8dp"
      android:layout_marginTop="16dp"
      android:background="@drawable/awhitelist"
      android:textColor="@android:color/white"
      android:textSize="18sp"
      android:visibility="invisible"
      app:layout_constraintBottom_toBottomOf="parent"
      app:layout_constraintEnd_toEndOf="parent"
      app:layout_constraintHorizontal_bias="0.266"
      app:layout_constraintStart_toStartOf="parent"
      app:layout_constraintTop_toTopOf="parent"
      app:layout_constraintVertical_bias="0.0" />

  <Button
      android:id="@+id/Save"
      android:layout_width="67dp"
      android:layout_height="62dp"
      android:layout_marginBottom="8dp"
      android:layout_marginEnd="8dp"
      android:layout_marginStart="8dp"
      android:layout_marginTop="16dp"
      android:background="@drawable/save"
      android:textColor="@android:color/white"
      android:textSize="18sp"
      app:layout_constraintBottom_toBottomOf="parent"
      app:layout_constraintEnd_toEndOf="parent"
      app:layout_constraintHorizontal_bias="0.0"
      app:layout_constraintStart_toStartOf="parent"
      app:layout_constraintTop_toTopOf="parent"
      app:layout_constraintVertical_bias="0.0" />

  <android.support.v7.widget.RecyclerView
      android:id="@+id/myrecycle"
      android:layout_width="match_parent"
      android:layout_height="797dp"
      android:layout_marginTop="14dp"
      app:layout_constraintBottom_toBottomOf="parent"
      app:layout_constraintEnd_toEndOf="parent"
      app:layout_constraintStart_toStartOf="parent">


  </android.support.v7.widget.RecyclerView>


  <ImageView
      android:id="@+id/appWindow"
      android:layout_width="wrap_content"
      android:layout_height="300dp"
      android:layout_marginBottom="8dp"
      android:layout_marginEnd="8dp"
      android:layout_marginStart="8dp"
      android:layout_marginTop="8dp"
      android:visibility="visible"
      app:layout_constraintBottom_toBottomOf="parent"
      app:layout_constraintEnd_toEndOf="parent"
      app:layout_constraintStart_toStartOf="parent"
      app:layout_constraintTop_toTopOf="parent"
      app:layout_constraintVertical_bias="0.496"
      app:srcCompat="@drawable/popupt" />

  <TextView
      android:id="@+id/apptextview"
      android:layout_width="wrap_content"
      android:layout_height="35dp"
      android:layout_marginBottom="8dp"
      android:layout_marginEnd="8dp"
      android:layout_marginStart="8dp"
      android:layout_marginTop="8dp"
      android:text="How long of innactivity before closing."
      android:textColor="@android:color/background_dark"
      android:textSize="14sp"
      app:layout_constraintBottom_toBottomOf="@+id/appWindow"
      app:layout_constraintEnd_toEndOf="@+id/appWindow"
      app:layout_constraintHorizontal_bias="0.0"
      app:layout_constraintStart_toStartOf="@+id/appWindow"
      app:layout_constraintTop_toTopOf="@+id/appWindow"
      app:layout_constraintVertical_bias="0.421" />

  <Button
      android:id="@+id/appokaybutton"
      android:layout_width="105dp"
      android:layout_height="37dp"
      android:layout_marginBottom="8dp"
      android:layout_marginEnd="8dp"
      android:layout_marginStart="8dp"
      android:layout_marginTop="8dp"
      android:background="@drawable/ok"
      android:visibility="visible"
      app:layout_constraintBottom_toBottomOf="@+id/appWindow"
      app:layout_constraintEnd_toEndOf="@+id/appWindow"
      app:layout_constraintHorizontal_bias="0.022"
      app:layout_constraintStart_toStartOf="@+id/appWindow"
      app:layout_constraintTop_toTopOf="@+id/appWindow"
      app:layout_constraintVertical_bias="0.724" />

  <Button
      android:id="@+id/appcancelbutton"
      android:layout_width="105dp"
      android:layout_height="37dp"
      android:layout_marginBottom="8dp"
      android:layout_marginEnd="8dp"
      android:layout_marginStart="8dp"
      android:layout_marginTop="8dp"
      android:background="@drawable/cancel2"
      app:layout_constraintBottom_toBottomOf="@+id/appWindow"
      app:layout_constraintEnd_toEndOf="@+id/appWindow"
      app:layout_constraintHorizontal_bias="0.96"
      app:layout_constraintStart_toStartOf="@+id/appWindow"
      app:layout_constraintTop_toTopOf="@+id/appWindow"
      app:layout_constraintVertical_bias="0.724" />

  <TextView
      android:id="@+id/appAppname"
      android:layout_width="266dp"
      android:layout_height="24dp"
      android:layout_marginBottom="220dp"
      android:layout_marginEnd="8dp"
      android:layout_marginStart="8dp"
      android:layout_marginTop="8dp"
      android:text="TextView"
      android:textColor="@android:color/background_dark"
      app:layout_constraintBottom_toBottomOf="@+id/appWindow"
      app:layout_constraintEnd_toEndOf="@+id/appWindow"
      app:layout_constraintHorizontal_bias="0.0"
      app:layout_constraintStart_toStartOf="@+id/appWindow"
      app:layout_constraintTop_toTopOf="@+id/appWindow"
      app:layout_constraintVertical_bias="0.958" />

  <ImageView
      android:id="@+id/appwhite"
      android:layout_width="55dp"
      android:layout_height="49dp"
      android:layout_marginBottom="8dp"
      android:layout_marginEnd="8dp"
      android:layout_marginStart="8dp"
      android:layout_marginTop="8dp"
      app:layout_constraintBottom_toBottomOf="@+id/appWindow"
      app:layout_constraintEnd_toEndOf="@+id/appWindow"
      app:layout_constraintHorizontal_bias="0.969"
      app:layout_constraintStart_toStartOf="@+id/appWindow"
      app:layout_constraintTop_toTopOf="@+id/appWindow"
      app:layout_constraintVertical_bias="0.391"
      app:srcCompat="@drawable/white" />

  <EditText
      android:id="@+id/apptimer"
      android:layout_width="55dp"
      android:layout_height="48dp"
      android:layout_marginBottom="8dp"
      android:layout_marginEnd="8dp"
      android:layout_marginStart="8dp"
      android:layout_marginTop="8dp"
      android:ems="10"
      android:inputType="number"
      app:layout_constraintBottom_toBottomOf="@+id/appWindow"
      app:layout_constraintEnd_toEndOf="@+id/appWindow"
      app:layout_constraintHorizontal_bias="0.969"
      app:layout_constraintStart_toStartOf="@+id/appWindow"
      app:layout_constraintTop_toTopOf="@+id/appWindow"
      android:windowSoftInputMode="stateAlwaysHidden"
      android:focusableInTouchMode="true"
      android:clickable="true"

      app:layout_constraintVertical_bias="0.389" />


  <Switch
      android:id="@+id/appswitch"
      android:layout_width="43dp"
      android:layout_height="39dp"
      android:layout_marginBottom="8dp"
      android:layout_marginEnd="8dp"
      android:layout_marginStart="8dp"
      android:layout_marginTop="8dp"
      android:textSize="14sp"
      app:layout_constraintBottom_toBottomOf="@+id/appWindow"
      app:layout_constraintEnd_toEndOf="@+id/appWindow"
      app:layout_constraintHorizontal_bias="0.925"
      app:layout_constraintStart_toStartOf="@+id/appWindow"
      app:layout_constraintTop_toTopOf="@+id/appWindow"
      app:layout_constraintVertical_bias="0.571" />

  <TextView
      android:id="@+id/addBlack"
      android:layout_width="wrap_content"
      android:layout_height="wrap_content"
      android:layout_marginBottom="8dp"
      android:layout_marginEnd="8dp"
      android:layout_marginStart="8dp"
      android:layout_marginTop="8dp"
      android:text="Add to Blacklist."
      android:textColor="@android:color/background_dark"
      app:layout_constraintBottom_toBottomOf="@+id/appWindow"
      app:layout_constraintEnd_toEndOf="@+id/appWindow"
      app:layout_constraintHorizontal_bias="0.0"
      app:layout_constraintStart_toStartOf="@+id/appWindow"
      app:layout_constraintTop_toTopOf="@+id/appWindow"
      app:layout_constraintVertical_bias="0.566" />

</android.support.constraint.ConstraintLayout>